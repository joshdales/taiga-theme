    "breadcrumb.activeSelectionForeground": "{{ light.blue }}",
    "breadcrumbPicker.background": "{{ ui.1 }}"
    // TODO: Symbol Icons Colours
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "{{ ui.text }}6c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.class meta.block",
        "keyword.operator.type.annotation",
        "keyword.operator.optional.",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic",
        "meta.type.annotation meta.brace",
        "meta.field.declaration"
      ],
      "settings": {
        "foreground": "{{ ui.text }}cb"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "punctuation.definition.variable",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "{{ main.green }}D9"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type.function",
        "storage.type",
        "storage.modifier",
        "punctuation.definition.keyword",
        "support.function"
      ],
      "settings": {
        "foreground": "{{ light.purple }}D9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "{{ light.green }}"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json",
        "punctuation.support.type.property-name",
        "meta.objectliteral meta.object-literal.key"
      ],
      "settings": {
        "foreground": "{{ main.green }}D9"
      }
    },
    {
      "name": "YAML Key",
      "scope": [
        "source.yaml entity.name.tag",
        "source.yaml punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "{{ main.green }}D9"
      }
    },
    {
      "name": "Class and Interface Names",
      "scope": [
        "meta.class",
        "meta.class entity.name",
        "meta.class entity.other.inherited-class",
        "meta.interface entity.name",
        "meta.interface entity.other.inherited-class",
        "support.class",
        "meta.module",
        "entity.name",
        "support.constant"
      ],
      "settings": {
        "foreground": "{{ light.yellow }}"
      }
    },
    {
      "name": "Type definitions",
      "scope": [
        "support.type",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "{{ light.yellow }}",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "meta.definition entity.name.function",
        "entity.name.function",
        "meta.function-call support.function.console"
      ],
      "settings": {
        "foreground": "{{ light.teal }}d5"
      }
    },
    {
      "name": "Tag Name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "{{ main.green }}D9"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "{{ ui.text }}bb"
      }
    },
    {
      "name": "Tag attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "{{ main.teal }}"
      }
    },
    {
      "name": "Console This",
      "scope": [
        "variable.language.this",
        "support.class.console",
        "variable.language.self.ruby",
        "support.variable.object"
      ],
      "settings": {
        "foreground": "{{ light.yellow }}",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "{{ light.purple }}D9"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "{{ main.teal }}"
      }
    },
    {
      "name": "CSS Class",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.css punctuation.definition.entity.css"
      ],
      "settings": {
        "foreground": "{{ main.teal }}"
      }
    },
    {
      "name": "CSS ID",
      "scope": [
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.id.css punctuation.definition.entity.css"
      ],
      "settings": {
        "foreground": "{{ light.teal }}d9"
      }
    },
    {
      "name": "CSS function",
      "scope": [
        "source.css support.function"
      ],
      "settings": {
        "foreground": "{{ light.teal }}d9"
      }
    },
    {
      "name": "CSS unit",
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "{{ main.teal }}"
      }
    },
    {
      "name": "CSS unit",
      "scope": [
        "support.constant.media"
      ],
      "settings": {
        "foreground": "{{ main.green }}D9"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "markup.heading punctuation.definition.heading",
        "markup.heading entity.name.section"
      ],
      "settings": {
        "foreground": "{{ main.green }}D9"
      }
    },
    {
      "name": "Markdown Italics",
      "scope": [
        "markup.italic",
        "markup.italic punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "{{ light.teal }}D9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "markup.bold punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "{{ light.purple }}D9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown List",
      "scope": [
        "markup.list punctuation.definition.list.begin"
      ],
      "settings": {
        "foreground": "{{ light.yellow }}",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Inline Code",
      "scope": [
        "markup.inline.raw",
        "markup.inline.raw punctuation.definition.raw"
      ],
      "settings": {
        "foreground": "{{ light.yellow }}"
      }
    },
    {
      "name": "Markdown Block Code",
      "scope": [
        "markup.fenced_code.block",
        "markup.fenced_code.block punctuation.definition"
      ],
      "settings": {
        "foreground": "{{ light.yellow }}"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote punctuation.definition"
      ],
      "settings": {
        "foreground": "{{ light.teal }}"
      }
    },
    {
      "name": "Markdown Seperator",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "{{ main.green }}D9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "meta.link.inline",
        "meta.link.inet",
        "meta.link.inet punctuation.definition.link",
        "meta.link.reference markup.underline.link",
        "meta.image.inline"
      ],
      "settings": {
        "foreground": "{{ main.green }}D9"
      }
    },
    {
      "name": "Ruby Symbol",
      "scope": [
        "source.ruby constant.other.symbol",
        "source.ruby constant.other.symbol punctuation"
      ],
      "settings": {
        "foreground": "{{ main.green }}D9"
      }
    },
    {
      "name": "Ruby Class",
      "scope": [
        "meta.class.ruby keyword.operator",
        "meta.module.ruby entity.other.inherited-class punctuation"
      ],
      "settings": {
        "foreground": "{{ light.yellow }}"
      }
    },
    {
      "name": "Ruby Hash Rocket",
      "scope": [
        "source.ruby punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "{{ main.purple }}"
      }
    },
    {
      "name": "Ruby Hash",
      "scope": [
        "source.ruby constant.language.symbol.ruby",
        "source.ruby punctuation.section.symbol.begin.ruby",
        "source.ruby punctuation.section.symbol.end.ruby",
        "source.ruby punctuation.definition.constant.ruby",
        "source.ruby constant.language.symbol.hashkey.ruby",
      ],
      "settings": {
        "foreground": "{{ main.teal }}",
      }
    }
  ]
}
