    // Settings Editor Colours
    "settings.modifiedItemIndicator": "{{ main.teal }}",
    "settings.focusedRowBackground": "{{ main.blue }}38",
    "notebook.focusedRowBorder": "{{ main.blue }}38",
    "notebook.rowHoverBackground": "{{ main.blue }}6c",
    // Breadcrumbs colours
    "breadcrumb.foreground": "{{ ui.text }}",
    "breadcrumb.background": "{{ ui.1 }}81",
