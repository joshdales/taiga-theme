		"gitDecoration.ignoredResourceForeground": "#D7FCFE6e",
		// errors
		"editorError.foreground": "#d1443f",
		"editorOverviewRuler.errorForeground": "#d1443f",
		"minimap.errorHighlight": "#d1443f",
		// titlebar
		"titleBar.activeBackground": "#152133",
		"titleBar.inactiveBackground": "#152133",
		// progress
		"progressBar.background": "#527CBF",
