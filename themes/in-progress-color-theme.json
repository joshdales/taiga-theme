		"editorSuggestWidget.highlightForeground": "#527CBF",
		"editorSuggestWidget.selectedBackground": "#1B2940",
		"editorSuggestWidget.foreground": "#D7FCFE",
		// link
		"textLink.foreground": "#3ED074",
		"textLink.activeForeground": "#3ed07481",
