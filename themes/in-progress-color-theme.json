		"notificationsErrorIcon.foreground": "#D1453F",
		"notificationsWarningIcon.foreground": "#d9a845",
		"notificationsInfoIcon.foreground": "#527CBF",
		// lightbulb
		"editorLightBulb.foreground": "#007a2c",
		// welcomePage
		"welcomePage.buttonBackground":"#152133",
		"welcomePage.buttonHoverBackground": "#15213381"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#d7fcfea6",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"meta.class meta.block",
				"keyword.operator.type.annotation",
				"keyword.operator.optional.",
				"keyword.operator.assignment",
				"keyword.operator.comparison",
				"keyword.operator.arithmetic"
			],
			"settings": {
				"foreground": "#d7fcfecb",
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#56F290d2",
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage.type.function",
				"storage.type",
				"storage.modifier",
				"punctuation.definition.keyword",
			],
			"settings": {
				"foreground": "#C775EA",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"punctuation.definition.string",
			],
			"settings": {
				"foreground": "#FAB97F"
			}
		},
		{
			"name": "JSON Key",
			"scope": [
				"source.json meta.structure.dictionary.json support.type.property-name.json",
				"punctuation.support.type.property-name"
			],
			"settings": {
				"foreground": "#C775EA"
			}
		},
		{
			"name": "Class and Interface Names",
			"scope": [
				"meta.class",
				"meta.class entity.name",
				"meta.class entity.other.inherited-class",
				"meta.interface entity.name",
				"support.class",
				"meta.module",
			],
			"settings": {
				"foreground": "#FAB267"
			}
		},

		{
			"name": "Type definitions",
			"scope": [
				"support.type",
				"meta.type.annotation"
			],
			"settings": {
				"foreground": "#FAB267",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"meta.definition entity.name.function",
				"entity.name.function",
				"meta.function-call support.function.console"
			],
			"settings": {
				"foreground": "#53ceffd5"
			}
		},
		{
			"name": "Tag Name",
			"scope": [
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#56F290d2",
			}
		},
		{
			"name": "Tag",
			"scope": [
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#3ed0747e",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Tag attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#E37474",
			}
		},
		{
			"name": "Console This",
			"scope": [
				"variable.language.this",
				"support.class.console",
				"variable.language.self.ruby"
			],
			"settings": {
				"foreground": "#FAB267",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Embedded",
			"scope": [
				"punctuation.section.embedded",
				"punctuation.definition.template-expression"
			],
			"settings": {
				"foreground": "#C775EA",
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#E37474",
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.css punctuation.definition.entity.css"
			],
			"settings": {
				"foreground": "#E37474",
			}
		},
		{
			"name": "CSS ID",
			"scope": [
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.id.css punctuation.definition.entity.css"
			],
			"settings": {
				"foreground": "#53ceffd5",
			}
		},
		{
			"name": "CSS function",
			"scope": [
				"source.css support.function",
			],
			"settings": {
				"foreground": "#53ceffd5",
			}
		},
		{
			"name": "CSS unit",
			"scope": [
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#E37474",
			}
		},
		{
			"name": "CSS unit",
			"scope": [
				"support.constant.media"
			],
			"settings": {
				"foreground": "#3ed074",
			}
		},
		{
			"name": "Markdown Headings",
			"scope": [
				"markup.heading",
				"markup.heading punctuation.definition.heading",
			],
			"settings": {
				"foreground": "#56F290"
			}
		},
		{
			"name": "Markdown Italics",
			"scope": [
				"markup.italic",
				"markup.italic punctuation.definition.italic",
			],
			"settings": {
				"foreground": "#53CEFF",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": [
				"markup.bold",
				"markup.bold punctuation.definition.bold",
			],
			"settings": {
				"foreground": "#C775EA",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown List",
			"scope": [
				"markup.list punctuation.definition.list.begin",
			],
			"settings": {
				"foreground": "#FAB97F",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Inline Code",
			"scope": [
				"markup.inline.raw",
				"markup.inline.raw punctuation.definition.raw",
			],
			"settings": {
				"foreground": "#FAB97F",
			}
		},
		{
			"name": "Markdown Block Code",
			"scope": [
				"markup.fenced_code.block",
				"markup.fenced_code.block punctuation.definition",
			],
			"settings": {
				"foreground": "#FAB97F",
			}
		},
		{
			"name": "Markdown Quote",
			"scope": [
				"markup.quote",
				"markup.quote punctuation.definition",
			],
			"settings": {
				"foreground": "#F37DAE",
			}
		},
		{
			"name": "Markdown Seperator",
			"scope": [
				"meta.separator.markdown",
			],
			"settings": {
				"foreground": "#56F290",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Link",
			"scope": [
				"meta.link.inline",
				"meta.link.inet",
				"meta.link.inet punctuation.definition.link",
				"meta.link.reference markup.underline.link",
				"meta.image.inline"
			],
			"settings": {
				"foreground": "#56F290",
			}
		},
	]
}