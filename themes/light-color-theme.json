  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#0d141fA6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.class meta.block",
        "keyword.operator.type.annotation",
        "keyword.operator.optional.",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic",
        "meta.type.annotation meta.brace",
        "meta.field.declaration",
        "string.template meta.brace"
      ],
      "settings": {
        "foreground": "#0d141fBF"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "punctuation.definition.variable",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#196339"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type.function",
        "storage.type",
        "storage.modifier",
        "punctuation.definition.keyword",
        "support.function"
      ],
      "settings": {
        "foreground": "#5d3284",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#114c2b"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json",
        "punctuation.support.type.property-name",
        "meta.objectliteral meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#196339"
      }
    },
    {
      "name": "YAML Key",
      "scope": [
        "source.yaml entity.name.tag",
        "source.yaml punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#196339"
      }
    },
    {
      "name": "Class and Interface Names",
      "scope": [
        "meta.class",
        "meta.class entity.name",
        "meta.class entity.other.inherited-class",
        "meta.interface entity.name",
        "meta.interface entity.other.inherited-class",
        "support.class",
        "meta.module",
        "entity.name",
        "support.constant"
      ],
      "settings": {
        "foreground": "#503c13"
      }
    },
    {
      "name": "Type definitions",
      "scope": [
        "support.type",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#503c13",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "meta.definition entity.name.function",
        "entity.name.function",
        "meta.function-call support.function.console"
      ],
      "settings": {
        "foreground": "#0b524d"
      }
    },
    {
      "name": "Tag Name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag support.class.component"
      ],
      "settings": {
        "foreground": "#155732"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#0d141fBF"
      }
    },
    {
      "name": "Tag attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#0b524d"
      }
    },
    {
      "name": "Console This",
      "scope": [
        "variable.language.this",
        "support.class.console",
        "variable.language.self.ruby",
        "support.variable.object"
      ],
      "settings": {
        "foreground": "#503c13",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#502a72"
      }
    },
    {
      "name": "General Constants",
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "#2e4974"
      }
    },
    {
      "name": "CSS Class",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.css punctuation.definition.entity.css",
        "entity.other.attribute-name.placeholder.css punctuation.definition.entity.css",
        "entity.other.attribute-name.placeholder.css"
      ],
      "settings": {
        "foreground": "#084642"
      }
    },
    {
      "name": "CSS ID",
      "scope": [
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.id.css punctuation.definition.entity.css"
      ],
      "settings": {
        "foreground": "#0b524d"
      }
    },
    {
      "name": "CSS function",
      "scope": [
        "source.css support.function",
        "entity.name.function.scss"
      ],
      "settings": {
        "foreground": "#502a72"
      }
    },
    {
      "name": "CSS unit",
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#2e4974"
      }
    },
    {
      "name": "CSS constant",
      "scope": [
        "support.constant.media"
      ],
      "settings": {
        "foreground": "#155732"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css",
        "meta.property-list.css entity.name.tag.css",
        "meta.property-list.scss entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#155732"
      }
    },
    {
      "name": "CSS Hex",
      "scope": [
        "punctuation.definition.constant.css"
      ],
      "settings": {
        "foreground": "#2e4974"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "markup.heading punctuation.definition.heading",
        "markup.heading entity.name.section"
      ],
      "settings": {
        "foreground": "#155732"
      }
    },
    {
      "name": "Markdown Italics",
      "scope": [
        "markup.italic",
        "markup.italic punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#084642",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "markup.bold punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "#502a72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown List",
      "scope": [
        "markup.list punctuation.definition.list.begin"
      ],
      "settings": {
        "foreground": "#503c13",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Inline Code",
      "scope": [
        "markup.inline.raw",
        "markup.inline.raw punctuation.definition.raw"
      ],
      "settings": {
        "foreground": "#503c13"
      }
    },
    {
      "name": "Markdown Block Code",
      "scope": [
        "markup.fenced_code.block",
        "markup.fenced_code.block punctuation.definition"
      ],
      "settings": {
        "foreground": "#503c13"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote punctuation.definition"
      ],
      "settings": {
        "foreground": "#084642"
      }
    },
    {
      "name": "Markdown Separator",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "#196339",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "meta.link.inline",
        "meta.link.inet",
        "meta.link.inet punctuation.definition.link",
        "meta.link.reference markup.underline.link",
        "meta.image.inline"
      ],
      "settings": {
        "foreground": "#155732"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote.markdown",
        "markup.quote.markdown punctuation.definition.quote.begin.markdown"
      ],
      "settings": {
        "foreground": "#2e4974",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Ruby Symbol",
      "scope": [
        "source.ruby constant.other.symbol",
        "source.ruby constant.other.symbol punctuation"
      ],
      "settings": {
        "foreground": "#155732"
      }
    },
    {
      "name": "Ruby Class",
      "scope": [
        "meta.class.ruby keyword.operator",
        "meta.module.ruby entity.other.inherited-class punctuation"
      ],
      "settings": {
        "foreground": "#503c13"
      }
    },
    {
      "name": "Ruby Hash Rocket",
      "scope": [
        "source.ruby punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#5d3284"
      }
    },
    {
      "name": "Ruby Hash",
      "scope": [
        "source.ruby constant.language.symbol.ruby",
        "source.ruby punctuation.section.symbol.begin.ruby",
        "source.ruby punctuation.section.symbol.end.ruby",
        "source.ruby punctuation.definition.constant.ruby",
        "source.ruby constant.language.symbol.hashkey.ruby",
        "constant.language.symbol.hashkey.ruby punctuation.definition.constant.hashkey.ruby"
      ],
      "settings": {
        "foreground": "#0b524d"
      }
    },
    {
      "name": "Ruby Method",
      "scope": [
        "entity.name.function.ruby"
      ],
      "settings": {
        "foreground": "#0b524d"
      }
    },
    {
      "name": "Ruby Keyword Arguments",
      "scope": [
        "meta.function.method.with-arguments.ruby constant.other.symbol.hashkey.parameter.function.ruby punctuation.definition.constant.ruby"
      ],
      "settings": {
        "foreground": "#155732"
      }
    },
    {
      "name": "JSX Tags",
      "scope": [
        "string.template meta.brace"
      ],
      "settings": {
        "foreground": "#0d141fBF"
      }
    }
  ]
}