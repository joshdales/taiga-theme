  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#0d141f80",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.class meta.block",
        "keyword.operator.type.annotation",
        "keyword.operator.optional.",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic",
        "meta.type.annotation meta.brace",
        "meta.field.declaration",
        "string.template meta.brace"
      ],
      "settings": {
        "foreground": "#0d141fBF"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "punctuation.definition.variable",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#0E8C46D9"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type.function",
        "storage.type",
        "storage.modifier",
        "punctuation.definition.keyword",
        "support.function"
      ],
      "settings": {
        "foreground": "#9345ADD9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#2DA644"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json",
        "punctuation.support.type.property-name",
        "meta.objectliteral meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#0E8C46D9"
      }
    },
    {
      "name": "YAML Key",
      "scope": [
        "source.yaml entity.name.tag",
        "source.yaml punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#0E8C46D9"
      }
    },
    {
      "name": "Class and Interface Names",
      "scope": [
        "meta.class",
        "meta.class entity.name",
        "meta.class entity.other.inherited-class",
        "meta.interface entity.name",
        "meta.interface entity.other.inherited-class",
        "support.class",
        "meta.module",
        "entity.name",
        "support.constant"
      ],
      "settings": {
        "foreground": "#C39840D9"
      }
    },
    {
      "name": "Type definitions",
      "scope": [
        "support.type",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#C39840D9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "meta.definition entity.name.function",
        "entity.name.function",
        "meta.function-call support.function.console"
      ],
      "settings": {
        "foreground": "#1A7B78"
      }
    },
    {
      "name": "Tag Name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag support.class.component"
      ],
      "settings": {
        "foreground": "#0E8C46D9"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#0d141fBF"
      }
    },
    {
      "name": "Tag attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#1A7B78"
      }
    },
    {
      "name": "Console This",
      "scope": [
        "variable.language.this",
        "support.class.console",
        "variable.language.self.ruby",
        "support.variable.object"
      ],
      "settings": {
        "foreground": "#C39840",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#9345ADD9"
      }
    },
    {
      "name": "General Constants",
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "#34428E"
      }
    },
    {
      "name": "CSS Class",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.css punctuation.definition.entity.css",
        "entity.other.attribute-name.placeholder.css punctuation.definition.entity.css",
        "entity.other.attribute-name.placeholder.css"
      ],
      "settings": {
        "foreground": "#1D8F89"
      }
    },
    {
      "name": "CSS ID",
      "scope": [
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.id.css punctuation.definition.entity.css"
      ],
      "settings": {
        "foreground": "#1D8F89BF"
      }
    },
    {
      "name": "CSS function",
      "scope": [
        "source.css support.function",
        "entity.name.function.scss"
      ],
      "settings": {
        "foreground": "#1A7B78"
      }
    },
    {
      "name": "CSS unit",
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#34428E"
      }
    },
    {
      "name": "CSS constant",
      "scope": [
        "support.constant.media"
      ],
      "settings": {
        "foreground": "#0E8C46D9"
      }
    },
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css",
        "meta.property-list.css entity.name.tag.css",
        "meta.property-list.scss entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#0E8C46D9"
      }
    },
    {
      "name": "CSS Hex",
      "scope": [
        "punctuation.definition.constant.css"
      ],
      "settings": {
        "foreground": "#34428E"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "markup.heading punctuation.definition.heading",
        "markup.heading entity.name.section"
      ],
      "settings": {
        "foreground": "#0E8C46D9"
      }
    },
    {
      "name": "Markdown Italics",
      "scope": [
        "markup.italic",
        "markup.italic punctuation.definition.italic"
      ],
      "settings": {
        "foreground": "#1D8F89D9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold",
        "markup.bold punctuation.definition.bold"
      ],
      "settings": {
        "foreground": "#9345ADD9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown List",
      "scope": [
        "markup.list punctuation.definition.list.begin"
      ],
      "settings": {
        "foreground": "#C39840",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Inline Code",
      "scope": [
        "markup.inline.raw",
        "markup.inline.raw punctuation.definition.raw"
      ],
      "settings": {
        "foreground": "#C39840"
      }
    },
    {
      "name": "Markdown Block Code",
      "scope": [
        "markup.fenced_code.block",
        "markup.fenced_code.block punctuation.definition"
      ],
      "settings": {
        "foreground": "#C39840"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote",
        "markup.quote punctuation.definition"
      ],
      "settings": {
        "foreground": "#1D8F89"
      }
    },
    {
      "name": "Markdown Separator",
      "scope": [
        "meta.separator.markdown"
      ],
      "settings": {
        "foreground": "#0E8C46D9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "meta.link.inline",
        "meta.link.inet",
        "meta.link.inet punctuation.definition.link",
        "meta.link.reference markup.underline.link",
        "meta.image.inline"
      ],
      "settings": {
        "foreground": "#0E8C46D9"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote.markdown",
        "markup.quote.markdown punctuation.definition.quote.begin.markdown"
      ],
      "settings": {
        "foreground": "#34428E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Ruby Symbol",
      "scope": [
        "source.ruby constant.other.symbol",
        "source.ruby constant.other.symbol punctuation"
      ],
      "settings": {
        "foreground": "#0E8C46D9"
      }
    },
    {
      "name": "Ruby Class",
      "scope": [
        "meta.class.ruby keyword.operator",
        "meta.module.ruby entity.other.inherited-class punctuation"
      ],
      "settings": {
        "foreground": "#C39840"
      }
    },
    {
      "name": "Ruby Hash Rocket",
      "scope": [
        "source.ruby punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#713889"
      }
    },
    {
      "name": "Ruby Hash",
      "scope": [
        "source.ruby constant.language.symbol.ruby",
        "source.ruby punctuation.section.symbol.begin.ruby",
        "source.ruby punctuation.section.symbol.end.ruby",
        "source.ruby punctuation.definition.constant.ruby",
        "source.ruby constant.language.symbol.hashkey.ruby",
        "constant.language.symbol.hashkey.ruby punctuation.definition.constant.hashkey.ruby"
      ],
      "settings": {
        "foreground": "#1A7B78"
      }
    },
    {
      "name": "Ruby Method",
      "scope": [
        "entity.name.function.ruby"
      ],
      "settings": {
        "foreground": "#1A7B78"
      }
    },
    {
      "name": "Ruby Keyword Arguments",
      "scope": [
        "meta.function.method.with-arguments.ruby constant.other.symbol.hashkey.parameter.function.ruby punctuation.definition.constant.ruby"
      ],
      "settings": {
        "foreground": "#0E8C46"
      }
    },
    {
      "name": "JSX Tags",
      "scope": [
        "string.template meta.brace"
      ],
      "settings": {
        "foreground": "#0d141fBF"
      }
    }
  ]
}