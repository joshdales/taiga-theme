  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5A6377",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.class meta.block",
        "keyword.operator.type.annotation",
        "keyword.operator.optional.",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic",
        "meta.type.annotation meta.brace",
        "meta.field.declaration",
        "string.template meta.brace"
      ],
      "settings": {
        "foreground": "#2D3547"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "punctuation.definition.variable",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#2C965D"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type.function",
        "storage.type",
        "storage.modifier",
        "punctuation.definition.keyword",
        "support.function"
      ],
      "settings": {
        "foreground": "#773CBF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#00572F"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json",
        "punctuation.support.type.property-name",
        "meta.objectliteral meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#2C965D"
      }
    },
    {
      "name": "YAML Key",
      "scope": [
        "source.yaml entity.name.tag",
        "source.yaml punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#2C965D"
      }
    },
    {
      "name": "Class and Interface Names",
      "scope": [
        "meta.class",
        "meta.class entity.name",
        "meta.class entity.other.inherited-class",
        "meta.interface entity.name",
        "meta.interface entity.other.inherited-class",
        "support.class",
        "meta.module",
        "entity.name",
        "support.constant"
      ],
      "settings": {
        "foreground": "#5F4200"
      }
    },
    {
      "name": "Type definitions",
      "scope": [
        "support.type",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#5F4200",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "meta.definition entity.name.function",
        "entity.name.function",
        "meta.function-call support.function.console"
      ],
      "settings": {
        "foreground": "#00736E"
      }
    },
    {
      "name": "Tag Name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag support.class.component"
      ],
      "settings": {
        "foreground": "#007742"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#2D3547"
      }
    },
    {
      "name": "Tag attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#00736E"
      }
    },
    {
      "name": "Console This",
      "scope": [
        "variable.language.this",
        "support.class.console",
        "variable.language.self.ruby",
        "support.variable.object"
      ],
      "settings": {
        "foreground": "#5F4200",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#5B169E"
      }
    },
    {
      "name": "General Constants",
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "#1762B6"
      }
    },
    {
      "name": "CSS Class",
